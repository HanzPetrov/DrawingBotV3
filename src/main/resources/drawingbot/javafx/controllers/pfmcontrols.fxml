<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefWidth="404.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="drawingbot.javafx.controllers.FXPFMControls">
   <VBox fillWidth="false" prefWidth="404.0" spacing="8.0">
      <children>
          <Label contentDisplay="RIGHT" graphicTextGap="8.0" prefHeight="25.0" prefWidth="403.0" text="Path Finding Module: ">
              <graphic>
                  <ComboBox fx:id="comboBoxPFM" prefHeight="25.0" prefWidth="246.0" visibleRowCount="100" />
              </graphic>
            <padding>
               <Insets left="8.0" />
            </padding>
            <VBox.margin>
               <Insets left="6.0" />
            </VBox.margin>
          </Label>
          <Separator prefHeight="3.0" prefWidth="412.0" />
         <HBox spacing="4.0">
            <children>
                <ComboBox fx:id="comboBoxPFMPreset" prefHeight="25.0" prefWidth="305.0" promptText="New Preset Name">
                  <HBox.margin>
                     <Insets left="6.0" />
                  </HBox.margin></ComboBox>
                <MenuButton fx:id="menuButtonPFMPresets" mnemonicParsing="false" text="Presets" />
            </children>
         </HBox>
      
          <TableView fx:id="tableViewAdvancedPFMSettings" editable="true" prefHeight="237.0" prefWidth="390.0" tableMenuButtonVisible="true">
              <columns>
                  <TableColumn fx:id="tableColumnSetting" editable="false" prefWidth="132.0" text="Setting" />
                  <TableColumn fx:id="tableColumnControl" prefWidth="137.0" text="Control" />
                  <TableColumn fx:id="tableColumnValue" prefWidth="89.0" text="Value" />
                  <TableColumn fx:id="tableColumnLock" prefWidth="45.0" text="Randomise Exclude" visible="false" />
              </columns>
          </TableView>
         <HBox prefHeight="25.0" spacing="8.0">
            <children>
                <Button fx:id="buttonPFMSettingReset" mnemonicParsing="false" prefHeight="25.0" prefWidth="117.0" text="Reset" />
                <Button fx:id="buttonPFMSettingRandom" mnemonicParsing="false" prefHeight="25.0" prefWidth="129.0" text="Randomise" />
                <Button fx:id="buttonPFMSettingHelp" mnemonicParsing="false" prefHeight="25.0" prefWidth="119.0" text="Help">
                  <HBox.margin>
                     <Insets />
                  </HBox.margin></Button>
            </children>
            <VBox.margin>
               <Insets left="6.0" />
            </VBox.margin>
         </HBox>
      
          <Separator prefHeight="3.0" prefWidth="412.0" />
      </children>
      <padding>
         <Insets left="8.0" top="8.0" />
      </padding>
   </VBox>
</AnchorPane>
